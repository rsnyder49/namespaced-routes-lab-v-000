<%= form_for @preference, url: admin_access_path(@preference) do |f| %>
  <div>
  Users can create new artist:<br>
  <%= f.radio_button :allow_create_artists, true %>Ascending
  <%= f.radio_button :allow_create_artists, false %>Descending
  </div>
  <div>
  Users can create new song:<br>
  <%= f.radio_button :allow_create_songs, true %>Ascending
  <%= f.radio_button :allow_create_songs, false %>Descending
  </div>
<% end %> 